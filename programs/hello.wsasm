; A simple hello world program
; This is a version written especially to test character literals
push 72 ; H
outchr
push '\x65' ; e
outchr
push '\154' ; l
outchr
push '\154';
outchr ; outchr another time for second 'L'
call print_o
push '\u002c'
outchr
push ' '
outchr
push 'W'
outchr
push 'o'
outchr
push 'r'
outchr
push 'l'
outchr
push 'd'
outchr
push '!'
outchr
push '\n'
outchr
slide 55
end ; end the program
label print_o
push 'o'
outchr
ret